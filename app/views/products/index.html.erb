
    <table>
        <tr>
            <th>title</th>
            <th>price</th>
            <th>category</th>
        </tr>
        <% @products.each do |product| %>
        <tr>
          <td><%= product.title %></td>
          <td><%= product.price %></td>
          <td><%= product.category.title %></td>
        </tr>
        <% end %>
    </table>
